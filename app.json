{
  "expo": {
    "name": "Manual",
    "slug": "ManualTechTask",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/manual_icon.png",
    "scheme": "myapp",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/manual_adaptive_icon.png",
        "backgroundColor": "#ECF0EB"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/manual_adaptive_icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ECF0EB"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
